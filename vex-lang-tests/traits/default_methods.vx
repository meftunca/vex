// Test file for Vex traits: Default Methods
// Note: Default methods may not be implemented in v0.1.2
// This file tests that traits compile with only required methods

trait Logger {
    fn log(msg: string)!;
    fn clear()!;
}

trait Calculator {
    fn add(x: i32, y: i32): i32;
    fn multiply(x: i32, y: i32): i32;
}

// Implementation with all required methods
struct ConsoleLogger impl Logger {
    prefix: string,

    fn log(msg: string)! {
        // Implementation
    }

    fn clear()! {
        // Implementation
    }
}

struct BasicCalculator impl Calculator {
    name: string,

    fn add(x: i32, y: i32): i32 {
        return x + y;
    }

    fn multiply(x: i32, y: i32): i32 {
        return x * y;
    }
}

fn main(): i32 {
    let! logger = ConsoleLogger { prefix: "Test" };
    logger.log("Message");
    logger.clear();

    let calc = BasicCalculator { name: "Calc" };
    let sum = calc.add(5, 3);
    let product = calc.multiply(4, 5);

    return sum + product;
}