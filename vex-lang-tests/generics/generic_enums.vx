// Test file for Vex generics: Generic Enums
// This file tests generic enum definitions and usage

enum Option<T> {
    Some(T),
    None,
}

enum Result<T, E> {
    Ok(T),
    Err(E),
}

enum Either<A, B> {
    Left(A),
    Right(B),
}

fn main(): i32 {
    // Generic enum instantiation
    let some_value = Option.Some(42);
    let none_value = Option.None;
    let some_string = Option.Some("hello");

    let success = Result.Ok(100);
    let failure = Result.Err("operation failed");
    let string_result = Result.Ok("success");

    let left_val = Either.Left(42);
    let right_val = Either.Right("hello");

    // Pattern matching with generic enums
    let option_value = match some_value {
        Option.Some(value) => { value }
        Option.None => { 0 }
    };

    let result_value = match success {
        Result.Ok(value) => { value }
        Result.Err(error) => { 0 }
    };

    let either_value = match left_val {
        Either.Left(num) => { num }
        Either.Right(str) => { 0 }
    };

    return option_value + result_value + either_value;
}