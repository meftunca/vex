// Test file for Vex operators: Arithmetic and Other Operators
// This file tests various operator categories

fn main(): i32 {
    // Arithmetic operators
    let a = 10 + 5;   // Addition
    let b = 10 - 5;   // Subtraction
    let c = 10 * 5;   // Multiplication
    let d = 10 / 5;   // Division
    let e = 10 % 3;   // Modulo

    // Comparison operators
    let f = a == b;   // Equal
    let g = a != b;   // Not equal
    let h = a < b;    // Less than
    let i = a <= b;   // Less or equal
    let j = a > b;    // Greater than
    let k = a >= b;   // Greater or equal

    // Logical operators
    let l = true && false;  // Logical AND
    let m = true || false;  // Logical OR
    let n = !true;          // Logical NOT

    // Bitwise operators
    let o = 5 & 3;    // Bitwise AND
    let p = 5 | 3;    // Bitwise OR
    let q = 5 ^ 3;    // Bitwise XOR
    let r = 5 << 1;   // Left shift
    let s = 5 >> 1;   // Right shift

    // Assignment operators
    let! t = 10;
    t = 20;           // Assignment
    t += 5;           // Add assign
    t -= 5;           // Subtract assign
    t *= 2;           // Multiply assign
    t /= 2;           // Divide assign
    t %= 3;           // Modulo assign
    t &= 3;           // Bitwise AND assign
    t |= 3;           // Bitwise OR assign
    t ^= 3;           // Bitwise XOR assign
    t <<= 1;          // Left shift assign
    t >>= 1;          // Right shift assign

    // Range operators
    let range1 = 0..10;   // Exclusive range
    let range2 = 0..=10;  // Inclusive range

    // Member access
    struct Point { x: i32, y: i32 }
    let point = Point { x: 1, y: 2 };
    let x_val = point.x;  // Dot operator

    return a + b + c + d + e + t + x_val;
}