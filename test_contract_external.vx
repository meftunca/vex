// Test contract enforcement with external methods

// This should trigger a warning: public method without contract
export struct Point {
    x: i32,
    y: i32
}

// ⚠️ WARNING: Public method 'distance' not in any contract
fn (p: &Point) distance(): f64 {
    return 0.0;
}

// ✅ OK: Private method (starts with _)
fn (p: &Point) _validate(): bool {
    return p.x >= 0 && p.y >= 0;
}

fn main() {
    let p = Point { x: 10, y: 20 };
    let d = p.distance();
    println(d);
}
