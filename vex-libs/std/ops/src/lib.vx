// std/ops - Operator Overloading Contracts
// Phase 1, Day 3-4: Arithmetic operators

contract Add {
    fn add(other: Self): Self;
}

contract Sub {
    fn sub(other: Self): Self;
}

contract Mul {
    fn mul(other: Self): Self;
}

contract Div {
    fn div(other: Self): Self;
}

contract Rem {
    fn rem(other: Self): Self;
}

// Builtin implementations
i32 extends Add, Sub, Mul, Div, Rem;
i64 extends Add, Sub, Mul, Div, Rem;
f64 extends Add, Sub, Mul, Div, Rem;
