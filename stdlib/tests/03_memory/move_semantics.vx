// Layer 1: Move Semantics - Basic Ownership Transfer
// Tests that non-Copy types move when assigned

struct Counter {
    value: i32,
}

fn consume(c: Counter): i32 {
    return c.value;
}

fn main(): i32 {
    // Test 1: Struct moves on assignment
    let c1 = Counter { value: 10 };
    let c2 = c1;  // c1 moves to c2
    // c1 is now invalid, cannot use it
    
    // Test 2: Struct moves into function
    let c3 = Counter { value: 20 };
    let result = consume(c3);  // c3 moves into function
    // c3 is now invalid
    
    // Test 3: Multiple sequential moves
    let c4 = Counter { value: 30 };
    let c5 = c4;
    let c6 = c5;
    
    return c6.value + result;  // 50
}
