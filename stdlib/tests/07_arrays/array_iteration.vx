// Layer 1: Array Iteration with For Loop
// Tests for-in loop with arrays

fn main(): i32 {
    // Test 1: Simple for loop over array
    let numbers = [1, 2, 3, 4, 5];
    let! sum = 0;
    
    for num in numbers {
        sum = sum + num;
    }
    // sum = 1+2+3+4+5 = 15
    
    // Test 2: For loop with array repeat
    let zeros = [0; 3];
    let! count = 0;
    
    for z in zeros {
        count = count + 1;
    }
    // count = 3
    
    return sum + count;  // 15 + 3 = 18
}
