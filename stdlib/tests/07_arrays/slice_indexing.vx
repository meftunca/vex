// Test: Slice indexing (&[T])
// Expected exit code: 4 (1 + 3)

fn main(): i32 {
    // Create array
    let arr = [1, 2, 3, 4, 5];
    
    // Create slice reference
    let slice: &[i32] = &arr;
    
    // Test indexing into slice
    let first = slice[0];
    let third = slice[2];
    
    return first + third;  // 1 + 3 = 4
}
