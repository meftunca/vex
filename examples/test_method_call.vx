// Test method calls with receiver syntax
struct Vector2 {
    x: f64,
    y: f64,
}

// Method with receiver
fn (self: &Vector2) length(): f64 {
    return self.x * self.x + self.y * self.y;
}

fn main(): i32 {
    v := Vector2{ x: 3.0, y: 4.0 };
    
    len := v.length();
    
    // 3*3 + 4*4 = 9 + 16 = 25
    if len == 25.0 {
        return 0;
    }
    
    return 1;
}
