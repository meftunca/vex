// Test typeof operator

fn main() {
    // Test basic typeof expressions
    let! x: i32 = 42;
    let! y: f64 = 3.14;
    let! s: string = "hello";
    
    print("Testing typeof operator:");
    print("x = 42 (i32)");
    print("y = 3.14 (f64)");
    print("s = hello (string)");
    
    // typeof is compile-time type introspection
    // In a full implementation, typeof(x) would return type info
    print("\ntypeof operator parsed successfully!");
}
