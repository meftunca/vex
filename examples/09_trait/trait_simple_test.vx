// Simple contract test - inline implementation without default methods
// struct ... impl contract { ... methods ... }

// 1. Define a contract with only required methods
contract Printer {
    print();
}

// 2. Struct with inline contract implementation
struct Message impl Printer {
    text: string,
    
    // Implement required method
    fn (self: &Message!) print() {
        // Print implementation
    }
}

fn main() {
    let! msg = Message { text: "Hello" };
    msg.print();
}
