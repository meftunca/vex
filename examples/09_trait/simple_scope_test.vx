// Simple test: global function call from method

fn add_numbers(a: i32, b: i32): i32 {
    return a + b;
}

struct Calculator {
    value: i32,
    
    fn (self: &Calculator!) test(): i32 {
        // Call global function
        let result = add_numbers(10, 20);
        return result;
    }
}

fn main(): i32 {
    let! calc = Calculator { value: 5 };
    let result = calc.test();
    print("Result: ", result);
    return result; // Should return 30
}
