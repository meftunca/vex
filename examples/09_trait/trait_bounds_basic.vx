// Test: Basic trait bounds parsing and compilation
// Expected: Compiles successfully (runtime checking not yet implemented)

trait Display {
    fn display(): string;
}

struct Point impl Display {
    x: i32,
    y: i32,
    fn (self: &Point)display(): string {
        return "Point";
    }
}

 

// Function with trait bound
fn print_value<T: Display>(value: T): i32 {
    return 42;
}

fn main(): i32 {
    let p = Point { x: 10, y: 20 };
    return print_value(p);
}
