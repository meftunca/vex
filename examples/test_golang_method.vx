// Test golang-style method with explicit receiver (for comparison)

struct Calculator {
    value: i32,
    
    // Explicit receiver (golang-style)
    fn (self: &Calculator) get_value(): i32 {
        return self.value;
    }
    
    fn (self: &Calculator!) set_value(new_val: i32) {
        self.value = new_val;
        return;
    }
    
    fn (self: &Calculator) add(x: i32): i32 {
        return self.value + x;
    }
}

fn main(): i32 {
    let! calc = Calculator { value: 10 };
    
    let result = calc.get_value();
    print("Initial value: ", result);
    
    calc.set_value(20);
    let result2 = calc.get_value();
    print("After set: ", result2);
    
    let sum = calc.add(5);
    print("After add: ", sum);
    
    return sum;
}
