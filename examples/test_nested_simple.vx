// Simple test: Non-generic nested struct

struct Inner {
    value: i32,
}

struct Outer {
    inner: Inner,
}

fn main(): i32 {
    let i = Inner { value: 42 };
    let o = Outer { inner: i };
    
    let val = o.inner.value;
    
    if val == 42 {
        println("✅ Non-generic nested works!");
        return 0;
    }
    
    println("❌ Failed");
    return 1;
}
