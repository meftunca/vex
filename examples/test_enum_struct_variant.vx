// Test: Enum Struct Variants  
// Status: ğŸš§ FUTURE FEATURE - Parser/codegen not implemented
// Struct variants like Circle { radius: i32 } need AST + codegen support

enum Shape {
    Point,
    Circle(i32),  // Tuple variant (supported)
}

fn main(): i32 {
    let circle = Shape.Circle(10);
    
    let area = match circle {
        Shape.Circle(radius) => radius * radius * 3,
        Shape.Point => 0,
    };
    
    println("Tuple variant works");
    return area;
}
