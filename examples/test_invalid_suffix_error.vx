// Test: Invalid ! suffix on immutable method

struct Counter {
    value: i32,
    
    fn get(): i32 {
        return self.value;
    }
}

fn main(): i32 {
    let counter = Counter { value: 42 };
    
    // âŒ ERROR: get() is immutable, cannot use !
    let val = counter.get()!;
    
    return val;
}
