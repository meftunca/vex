// Minimal format() test  
// Tests zero-cost type-safe formatting
// Note: println and i32_to_string are builtin functions

fn main(): i32 {
    println("Testing format()...");
    
    // Test 1: Format with single integer
    println("Value: {}", 42);
    
    // Test 2: Format with multiple args
    println("x={}, y={}", 10, 20);
    
    // Test 3: Format with mixed types
    println("int={}, float={}", 100, 3.14);
    
    // Test 4: Variadic print mode
    println("Hello", "World");
    
    println("Format test complete!");
    return 0;
}
