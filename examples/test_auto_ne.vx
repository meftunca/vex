// Test auto-generation of op!= from op==
// Expected: op!= should be auto-generated when only op== is implemented

contract Eq {
    op==(other: Self): bool;
    op!=(other: Self): bool;
}

struct Point impl Eq {
    x: i32,
    y: i32,
}

// Only implement op==, op!= should be auto-generated
fn (self: Point) op==(other: Point): bool {
    return self.x == other.x && self.y == other.y;
}

fn main(): i32 {
    let p1 = Point { x: 10, y: 20 };
    let p2 = Point { x: 10, y: 20 };
    let p3 = Point { x: 5, y: 15 };
    
    // Test op== (manually implemented)
    if !(p1 == p2) {
        return 1; // Should be equal
    }
    
    if p1 == p3 {
        return 2; // Should not be equal
    }
    
    // Test op!= (auto-generated from op==)
    if p1 != p2 {
        return 3; // Should be equal (not not-equal)
    }
    
    if !(p1 != p3) {
        return 4; // Should be not-equal
    }
    
    return 0; // Success
}
