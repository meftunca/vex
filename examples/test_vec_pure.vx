// Pure Vex Vec<T> test - Zero-cost abstractions
fn main():i32 {
    let v: Vec<i32> = Vec<i32>();
    v.push(10);
    v.push(20);
    v.push(30);
    
    println("Vec length: {}", v.len());
    println("Vec capacity: {}", v.capacity());
    
    println("v[0] = {}", v.get(0));
    println("v[1] = {}", v.get(1));
    println("v[2] = {}", v.get(2));
    
    println("Vec test passed!");
    return 0;
}
