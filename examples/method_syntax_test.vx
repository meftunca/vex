struct Counter {
    value: i32,
}

// Constructor
fn create_counter(): Counter {
    return Counter { value: 0 };
}

// Helper method
fn (self: &Counter) get_value(): i32 {
    return self.value;
}

// Method that calls another method without self. prefix
fn (self: &Counter!) increment(): i32 {
    // This should be parsed as self.get_value() (explicit method call)
    let current = self.get_value();
    let new_value = current + 1;
    return new_value;
}

fn main(): i32 {
    let counter = create_counter();
    let result = counter.increment();
    return result;
}