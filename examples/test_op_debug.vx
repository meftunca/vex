contract Add {
    op+(rhs: Self): Self;
}

struct Vec2 impl Add {
    x: f32,
    y: f32,
    
    op+(other: Vec2): Vec2 {
        print("In op+: self.x = {}, other.x = {}", self.x, other.x);
        let result_x = self.x + other.x;
        let result_y = self.y + other.y;
        print("Result: x = {}, y = {}", result_x, result_y);
        return Vec2 { x: result_x, y: result_y };
    }
}

fn main(): i32 {
    print("Creating v1...");
    let v1 = Vec2 { x: 1.0, y: 2.0 };
    print("Creating v2...");
    let v2 = Vec2 { x: 3.0, y: 4.0 };
    
    print("Calling v1 + v2...");
    let v3 = v1 + v2;
    
    print("Done! v3.x = {}", v3.x);
    return 42;
}
