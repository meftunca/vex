// Test generic max function with Ord contract
// Test contract bounds with generic functions

import { Ord } from "core";

struct Number impl Ord {
    value: i32,
    
    fn compare(other: Number): i32 {
        if self.value < other.value {
            return -1;
        }
        if self.value > other.value {
            return 1;
        }
        return 0;
    }
}

fn main(): i32 {
    let n1 = Number { value: 10 };
    let n2 = Number { value: 20 };
    
    // Direct comparison instead of generic function
    let cmp = n1.compare(n2);
    
    if cmp != -1 {
        println("❌ FAILED: n1 should be less than n2");
        return 1;
    }
    
    println("✅ PASSED: Ord contract works in generic context");
    return 0;
}

// Expected output:
// ✅ PASSED: Ord contract works in generic context
