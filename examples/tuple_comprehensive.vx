fn main(): i32 {
    // Two-element integer tuple
    coords := (10, 20);
    
    // Three-element mixed-type tuple
    person := (42, "Alice", true);
    
    // Empty tuple (unit type)
    unit := ();
    
    // Single-element tuple with trailing comma
    single := (100,);
    
    // Nested tuple
    nested := ((1, 2), (3, 4));
    
    return 0;
}
