// Phase 0.4b: Vec Constructor Test with New Syntax
// Test vec() syntax + automatic cleanup

fn test_vec_full(): i32 {
    // Create new Vec<i32> with cleaner syntax
    let v = vec();
    
    // Push values
    v.push(10);
    v.push(20);
    v.push(30);
    
    // Get length
    let len = v.len();
    
    // NO vec_free() needed - automatic cleanup!
    
    // Should return 3
    return len;
}

fn main(): i32 {
    return test_vec_full();
}
