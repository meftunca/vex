contract Add {
    op+(rhs: Self): Self;
}

struct Vec2 {
    x: f32,
    y: f32,
}

fn (self: Vec2) op+(other: Vec2): Vec2 {
    print("External op+: self.x = {}, other.x = {}", self.x, other.x);
    return Vec2 { x: self.x + other.x, y: self.y + other.y };
}

fn main(): i32 {
    let v1 = Vec2 { x: 1.0, y: 2.0 };
    let v2 = Vec2 { x: 3.0, y: 4.0 };
    
    let v3 = v1 + v2;
    
    print("Result: v3.x = {}", v3.x);
    return 42;
}
