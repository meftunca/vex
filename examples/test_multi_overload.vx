extern {
    fn malloc(size: i64): i64;
    fn free(ptr: i64);
}

fn abs(x: i8): i8 {
    if x < 0 {
        return 0 - x;
    }
    return x;
}

fn abs(x: i16): i16 {
    if x < 0 {
        return 0 - x;
    }
    return x;
}

fn main() {
    let r1: i8 = abs(5);
    let r2: i16 = abs(500);
    println("i8 abs: {}, i16 abs: {}", r1, r2);
}
