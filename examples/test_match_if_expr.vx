fn test_match(x: i8): i8 {
    if x == 0 {
        return 1;
    } elif x == 1 {
        return 2;
    } else {
        return 127;
    }
}

fn test_if_expr(cond: bool): i8 {
    if cond {
        return 10;
    } else {
        return 20;
    }
}

fn main() {
    println("Match result: {}", test_match(1));
    println("If expr result: {}", test_if_expr(true));
}
