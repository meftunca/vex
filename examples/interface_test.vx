// Simple interface without generics
interface Writer {
    fn write(data: i32): i32 {}
}

// Generic interface
interface Cache<K, V> {
    fn get(key: K): V {}
    fn set(key: K, value: V): i32 {}
}

// Struct that implements Writer
struct File {
    path: string,
}

// Method implementing Writer interface (implicit)
fn (self: &File) write(data: i32): i32 {
    return data;
}

fn main(): i32 {
    f := File{ path: "test.txt" };
    result := f.write(42);
    return result;
}
