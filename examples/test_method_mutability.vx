// Test: Method-level mutability syntax parsing

struct Counter {
    value: i32,
    
    // Immutable method (no !)
    fn get(): i32 {
        return self.value;
    }
    
    // Mutable method (with !)
    fn increment()! {
        self.value = self.value + 1;
    }
    
    // Mutable method with return
    fn add(x: i32)!: i32 {
        self.value = self.value + x;
        return self.value;
    }
}


fn main(): i32 {
    let! counter = Counter { value: 0 };
    
    print("Initial:", counter.get());
    
    counter.increment()!;
    print("After increment:", counter.get());
    
    counter.add(5)!;
    print("After add(5):", counter.get());
    
    return 0;
}
