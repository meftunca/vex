// Test static method syntax: Type.new()
// Static methods are called on type names, not instances

struct Point {
    x: i32,
    y: i32,
}

// Static constructor function (standalone)
fn point_new(x: i32, y: i32): Point {
    let result = Point { x: x, y: y };
    return result;
}

