// Debug test: Match on function result
// Expected: Returns 50

enum MyResult {
    Ok(i32),
    Err(i32)
}

fn calculate(x: i32): MyResult {
    return MyResult.Ok(x * 10);
}

fn main(): i32 {
    let! result = calculate(5);
    
    let output = match result {
        MyResult.Ok(val) => val,
        MyResult.Err(code) => code
    };
    
    return output;
}
