// Higher-order functions test - Basic function type parsing
// NOTE: Full implementation pending - this tests type parsing only

fn double(n: i32): i32 {
    return n * 2;
}

fn square(n: i32): i32 {
    return n * n;
}

// Function type is now parsed correctly: fn(i32) -> i32
// But codegen for passing functions as values not yet implemented
fn main(): i32 {
    let result1 = double(5);  // 10
    let result2 = square(5);  // 25
    return result1 + result2; // 35
}
