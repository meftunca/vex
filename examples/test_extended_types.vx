// Test i128, u128, f16, f128 support
// These types are implemented but may have platform-specific linker issues

fn main() {
    // Try i128
    let val_i128: i128 = 100;
    println("i128 value created successfully");
    
    // Try u128
    let val_u128: u128 = 200;
    println("u128 value created successfully");
    
    // Try f16
    let val_f16: f16 = 3.14;
    println("f16 value created successfully");
    
    // Try f128
    let val_f128: f128 = 2.71828;
    println("f128 value created successfully");
    
    println("All extended precision types work!");
}
