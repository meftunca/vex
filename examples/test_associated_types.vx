struct IntBox {
    value: i32,
    
    fn get(): Option<i32> {
        return Some(self.value);
    }
}

fn main(): i32 {
    let! int_box = IntBox { value: 42 };
    let result = int_box.get();
    
    return match result {
        Some(v) => v,
        None => -1,
    };
}
