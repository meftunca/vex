fn main() {
    let arr = [1, 2, 3, 4, 5];
    
    match arr {
        [first, second, ...rest] => {
            println("Rest pattern works!");
            println("First element matched");
            println("Second element matched");
        },
        _ => println("No match"),
    };
    
    // Test with just first element
    match arr {
        [head, ...tail] => {
            println("Head matched!");
            println("Tail exists!");
        },
        _ => println("Empty"),
    };
}
