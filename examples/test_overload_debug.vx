// Test function overloading with explicit types

export fn abs(x: i32): i32 {
    if x < 0 {
        return 0 - x;
    }
    return x;
}

export fn abs(x: f64): f64 {
    if x < 0.0 {
        return 0.0 - x;
    }
    return x;
}

fn main() {
    let a: i32 = abs(-5);
    let b: f64 = abs(-3.14);
    
    println("i32 abs: ");
    println(a);
    println("f64 abs: ");
    println(b);
}
