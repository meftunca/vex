// Simplified struct destructuring test

struct Point {
    x: i32,
    y: i32,
}

fn main() {
    let! p: Point = Point { x: 5, y: 10 };
    
    print("Testing basic struct destructuring:");
    
    // Simple match with struct pattern
    let! result: i32 = match p {
        Point { x: 0, y: 0 } => 0,
        Point { x, y } => x + y
    };
    
    print("Result: 15 expected");
    
    print("âœ… Basic struct destructuring test passed!");
}
