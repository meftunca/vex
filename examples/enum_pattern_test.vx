// Test enum pattern matching
enum Color {
    Red,
    Green,
    Blue,
}

fn main() {
    // Test 1: Red variant
    c1 := Color_Red();
    result1 := match c1 {
        Red => 1,
        Green => 2,
        Blue => 3,
        _ => 0,
    };
    print(result1);  // Should print 1
    
    // Test 2: Green variant
    c2 := Color_Green();
    result2 := match c2 {
        Red => 10,
        Green => 20,
        Blue => 30,
        _ => 0,
    };
    print(result2);  // Should print 20
    
    // Test 3: Blue variant
    c3 := Color_Blue();
    result3 := match c3 {
        Red => 100,
        Green => 200,
        Blue => 300,
        _ => 0,
    };
    print(result3);  // Should print 300
}
