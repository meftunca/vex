// Test 1: Default type parameters - Explicit override
// Scenario: Explicitly provide type arg that differs from default

contract Add<Rhs = Self> {
    op+(other: Rhs): Self;
}

struct Point impl Add<i32> {  // âœ… Explicit: Rhs = i32 (not Self)
    x: i32,
    y: i32,

    fn (self: &Point!) op+(other: i32): Point {
        return Point { 
            x: self.x + other, 
            y: self.y + other 
        };
    }
}

fn main(): i32 {
    let p = Point { x: 10, y: 20 };
    
    // Test Add<i32> (explicit override)
    let p2 = p + 5;
    
    // Verify: (10+5, 20+5) = (15, 25)
    if p2.x == 15 {
        if p2.y == 25 {
            return 0;  // Success
        }
    }
    
    return 1;  // Failure
}
