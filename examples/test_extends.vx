// Test composition pattern (Vex uses composition over inheritance)
// This demonstrates embedding structs instead of using 'extends'

struct Base {
    id: i32,
    name: string,
}

struct Derived {
    base: Base,
    extra: string,
}

fn main(): i32 {
    let obj = Derived {
        base: Base { id: 1, name: "test" },
        extra: "Test composition pattern (Vex uses composition over inheritance)",
    };
    
    println("ID: ");
    println(obj.base.id);
    println("Name: ");
    println(obj.base.name);
    println("Extra: ");
    println(obj.extra);
    
    return 0;
}
