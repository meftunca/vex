// Test inline struct method deprecation warnings

struct Vec2 {
    x: f32,
    y: f32,
    
    // This should trigger warning W0001
    add(other: Vec2): Vec2 {
        return Vec2 { x: self.x + other.x, y: self.y + other.y };
    }
}

struct Point impl Display {
    x: i32,
    y: i32,
    
    // This should also trigger warning W0001
    (self: &Point) display(): String {
        return format("({}, {})", self.x, self.y);
    }
}

fn main() {
    let v1 = Vec2 { x: 1.0, y: 2.0 };
    let v2 = Vec2 { x: 3.0, y: 4.0 };
    let v3 = v1.add(v2);
    println("v3: ({}, {})", v3.x, v3.y);
    
    let p = Point { x: 10, y: 20 };
    println(p.display());
}
