// Combined test without println
fn main(): i32 {
    // Test 1
    let add = |x, y| { x + y };
    let r1 = add(5, 10);
    if r1 != 15 { return 1; }
    
    // Test 2
    let mul = |a, b| a * b;
    let r2 = mul(3, 4);
    if r2 != 12 { return 2; }
    
    // Test 3
    let double = |n| n * 2;
    let r3 = double(7);
    if r3 != 14 { return 3; }
    
    // Test 4
    let get_five = || 5;
    let r4 = get_five();
    if r4 != 5 { return 4; }
    
    // Test 5
    let sub = |x, y|: i32 { x - y };
    let r5 = sub(20, 5);
    if r5 != 15 { return 5; }
    
    // Test 6
    let triple = |n|: i32 { n * 3 };
    let r6 = triple(5);
    if r6 != 15 { return 6; }
    
    return 0;
}
