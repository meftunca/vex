// Test bitwise operators

fn test_bitwise(): i32 {
    let a = 5;
    let b = 3;
    
    // Bitwise AND
    let c = a & b;
    
    // Bitwise OR
    let d = a | b;
    
    // Bitwise XOR
    let e = a ^ b;
    
    // Left shift
    let f = a << 1;
    
    // Right shift
    let g = a >> 1;
    
    return c + d + e + f + g;
}

fn main(): i32 {
    return test_bitwise();
}
