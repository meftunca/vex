fn add(a: i32, b: i32) :i32 {
    return a + b;
}

fn sub(a: i32, b: i32) :i32 {
    return a - b;
}

fn mul(a: i32, b: i32) :i32 {
    return a * b;
}

fn div(a: i32, b: i32) :i32 {
    return a / b;
}

fn main() :i32 {
    print(add(5, 8));
    print(sub(10, 3));
    print(mul(5, 6));
    print(div(10, 3));
    return 0;
}
