// Test: Trait bounds enforcement - POSITIVE TEST (should compile successfully)
// Demonstrates that trait bounds are checked and enforced correctly.
trait Display {
    fn display(self: &Self): String;
}

// Generic function with trait bound
fn print_value<T: Display>(value: T) {
    let s: String = value.display();
}

struct Point impl Display {
    x: i32,
    y: i32,
    
    fn display(self: &Point): String {
        return "Point";
    }
}

fn main(): i32 {
    let p = Point { x: 1, y: 2 };
    print_value(p);  // âœ… Works - Point implements Display
    
    return 0;
}
