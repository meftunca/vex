// Test: Immutable method trying to mutate (should fail in borrow checker)

struct Counter {
    value: i32,
    
    fn try_increment() {
        // This should be caught by borrow checker:
        // Immutable method cannot mutate fields
        self.value = self.value + 1;
    }
}

fn main(): i32 {
    let! counter = Counter { value: 0 };
    return 0;
}
