// For Loop Basic Test
// Test C-style for loops work
// Syntax: for init; condition; post { body }

fn main(): i32 {
    // Test 1: Basic for loop
    let! sum = 0;
    let! i = 0;
    for i = 0; i < 5; i = i + 1 {
        sum = sum + i;
    }
    // sum should be 0+1+2+3+4 = 10
    
    return sum;
}

