// Test: Associated Types in Traits
// Full implementation with type bindings

contract Container {
    type Item;  // ✅ Associated type declaration
    fn size(): i32;
}

struct IntBox impl Container {
    value: i32,
    type Item = i32;  // ✅ Associated type binding
    
    fn size(): i32 {
        return 4;  // sizeof(i32)
    }
}

fn main(): i32 {
    let int_box = IntBox { value: 42 };
    println("✅ Associated types parsed successfully!");
    return int_box.size();
}
