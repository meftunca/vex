// Test: Check if vex_vec operations work correctly

extern "C" {
    fn vex_vec_new(elem_size: i64): &void!;
    fn vex_vec_push(vec: &void!, elem: &i32);
    fn vex_vec_len(vec: &void): i64;
}

fn main(): i32 {
    let v1 = vex_vec_new(4);
    println("After new, len: {}", vex_vec_len(v1));
    
    let x1 = 100;
    vex_vec_push(v1, &x1);
    println("After push 1, len: {}", vex_vec_len(v1));
    
    let x2 = 200;
    vex_vec_push(v1, &x2);
    println("After push 2, len: {}", vex_vec_len(v1));
    
    let x3 = 300;
    vex_vec_push(v1, &x3);
    println("After push 3, len: {}", vex_vec_len(v1));
    
    return 0;
}
