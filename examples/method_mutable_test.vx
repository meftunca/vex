// Test mutable receiver methods
struct Counter {
    value: i32,
}

// Immutable method - reads value
fn (self: &Counter) get(): i32 {
    return self.value;
}

// Mutable method - modifies value (using *Counter for mutation)
fn (self: &Counter) increment(): i32 {
    // For now, just return incremented value
    // True mutation requires & vs * distinction in codegen
    return self.value + 1;
}

fn main(): i32 {
    c := Counter { value: 10 };
    
    // Call immutable method
    val := c.get();
    
    // Call mutable method  
    new_val := c.increment();
    
    return new_val; // Should return 11
}
