// Method syntax test
// Test method calls work properly

struct Point {
    x: i32,
    y: i32,
}

// Golang-style methods
fn (self: &Point) get_x(): i32 {
    return self.x;
}

fn (self: &Point) get_y(): i32 {
    return self.y;
}

fn (self: &Point) add(other: &Point): i32 {
    return self.x + other.x + self.y + other.y;
}

// Helper function to create Point
fn create_point(x: i32, y: i32): Point {
    return Point { x: x, y: y };
}

fn main(): i32 {
    let p1 = Point { x: 10, y: 20 };
    let p2 = Point { x: 5, y: 3 };
    
    // Method calls
    let x = p1.get_x();  // 10
    let y = p2.get_y();  // 3
    let sum = p1.add(&p2);  // 10+5+20+3 = 38
    
    return x + y + sum;  // 10 + 3 + 38 = 51
}
