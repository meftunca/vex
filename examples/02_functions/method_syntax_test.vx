// Method syntax test
// Test method calls work properly
trait Pointxx {
    fn new(x: i32, y: i32): Point {
        return Point { x: x, y: y };
    }
    
    fn get_x(self: &Point): i32 {
        return self.x;
    }
    
    fn get_y(self: &Point): i32 {
        return self.y;
    }
    
    fn add(self: &Point, other: &Point): i32 {
        return self.x + other.x + self.y + other.y;
    }
}
struct Point impl Pointxx{
    x: i32,
    y: i32,
}



fn main(): i32 {
    let p1 = Point.new(10, 20);
    let p2 = Point.new(5, 3);
    
    // Method calls
    let x = p1.get_x();  // 10
    let y = p2.get_y();  // 3
    let sum = p1.add(&p2);  // 10+5+20+3 = 38
    
    return x + y + sum;  // 10 + 3 + 38 = 51
}
