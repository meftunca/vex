// Method syntax test
// Test method calls work properly
trait Pointxx {
    fn create(x: i32, y: i32): Point {
        return Point { x: x, y: y };
    }
    
    fn (self: &Point)get_x(): i32 {
        return self.x;
    }
    
    fn (self: &Point)get_y(): i32 {
        return self.y;
    }
    
    fn (self: &Point)add( other: &Point): i32 {
        return self.x + other.x + self.y + other.y;
    }
}
struct Point impl Pointxx{
    x: i32,
    y: i32,
}



fn main(): i32 {
    let p1 = Point.create(10, 20);
    let p2 = Point.create(5, 3);
    
    // Method calls
    let x = p1.get_x();  // 10
    let y = p2.get_y();  // 3
    let sum = p1.add(&p2);  // 10+5+20+3 = 38
    
    return x + y + sum;  // 10 + 3 + 38 = 51
}
