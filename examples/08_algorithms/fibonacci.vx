// Fibonacci Sequence
// Classic recursive implementation
// Returns the nth Fibonacci number

fn fib(n: i32) : i32 {
    if n <= 1 {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

fn main() : i32 {
    // Calculate 10th Fibonacci number
    let result = fib(10);  // Should be 55
    return result;
}
