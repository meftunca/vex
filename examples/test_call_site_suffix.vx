// Test: Method call with ! suffix parsing

struct Counter {
    value: i32,
    
    fn get(): i32 {
        return self.value;
    }
    
    fn increment()! {
        self.value = self.value + 1;
    }
}

fn main(): i32 {
    let! counter = Counter { value: 0 };
    
    print("Initial:", counter.get());
    
    // Call with ! suffix (parsed but not yet enforced)
    counter.increment()!;
    
    print("After increment:", counter.get());
    
    return 0;
}
