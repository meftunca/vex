contract Add {
    op+(rhs: Self): Self;
}

struct Point impl Add {
    x: i32,
    y: i32,

    fn op+(other: Point): Point {
        return Point {
            x: self.x + other.x,
            y: self.y + other.y,
        };
    }
}

fn main(): i32 {
    let p1 = Point { x: 1, y: 2 };
    let p2 = Point { x: 3, y: 4 };
    let p3 = p1 + p2;  // Should call op+

    print(p3.x);  // Should print 4
    print(p3.y);  // Should print 6

    return 0;
}