// Time Module Test
// Tests time operations

import { println } from "io";
import { sleep, monotonic_ns } from "time";

fn main(): i32 {
    println("=== Time Module Test ===");
    
    // Test monotonic time
    let start: u64 = monotonic_ns();
    println("✓ monotonic_ns() working");
    
    // Test sleep (1ms = 1000000ns)
    let one_ms: i64 = 1000000;
    sleep(one_ms);
    println("✓ sleep() working");
    
    let end: u64 = monotonic_ns();
    println("✓ Time operations verified");
    
    println("✓ Time module working");
    return 0;
}
